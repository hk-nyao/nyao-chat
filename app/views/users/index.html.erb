<h1>Users#index</h1>
<body>
  <table>
    <thead>
      <tr>
        <th>name<th>
        <th>email<th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td> <%= user.name %> </td>
          <td> <%= user.email %> </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</body>
