<h1>User list</h1>
<body>
  <div class='col-xs-9'>
    <table class='table'>
      <thead>
        <tr>
          <th>name<th>
          <th>email<th>
          <th colspan="3"></th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td>
              <a href="/users/<%= user.id %>">
                <%= user.name %>
              </a>
            </td>
            <td></td>
            <td> <%= user.email %> </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</body>
