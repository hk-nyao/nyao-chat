<div class='container'>
<h1>User list</h1>
<body>
  <div class='col-xs-9'>
    <table class='table'>
      <thead class="table">
        <tr>
          <th>User name<th>
          <th>Introduction<th>
          <th colspan="3"></th>
        </tr>
      </thead>
      <tbody class=table>
        <% @users.each do |user| %>
          <tr>
            <td>
              <a href="/users/<%= user.id %>">
                <%= user.name %>
              </a>
            </td>
            <td></td>
            <td> <%= user.introduction %> </td>
            <td></td>
            <td></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</body>
</div>
